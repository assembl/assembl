{
    "_default": {
        "id": false,
        "type": false,
        "@id": "self",
        "@type": "&external_typename",
        "@view": "@view",
        "_default": false
    },
    "Permission": {
        "name": true
    },
    "AgentProfile": {
        "description": false,
        "preferred_email": false,
        "name": "&display_name"
    },
    "User": {
        "@extends": "AgentProfile",
        "creation_date": true,
        "last_login": true,
        "login_failures": false,
        "password": false
    },
    "UserTemplate": {
        "@extends": "User",
        "discussion": true,
        "for_role": true
    },
    "EmailAccount": false,
    "IdentityProviderAccount": false,
    "IdentityProvider": {
        "name": true,
        "provider_type": true,
        "trust_emails": true
        },
    "ContentSource": {
        "discussion_id": "discussion",
        "name": true,
        "creation_date": true
    },
    "PostSource": {
        "@extends": "ContentSource",
        "last_import": true
    },
    "AnnotatorSource": {
        "@extends": "ContentSource"
    },
    "AbstractMailbox": {
        "@extends": "PostSource",
        "admin_sender": true
    },
    "IMAPMailbox": {
        "@extends": "AbstractMailbox",
        "folder": true,
        "host": true,
        "port": true
    },
    "MailingList": {
        "@extends": "IMAPMailbox",
        "post_email_address": true
    },
    "Content": {
        "discussion": true,
        "date": "creation_date",
        "hidden": true,
        "import_date": false
    },
    "Post": {
        "@extends": "Content",
        "message_id": false,
        "ancestry": false,
        "parentId": "parent",
        "idCreator": "creator",
        "subject": "&get_title",
        "body": "&get_body_preview"
    },
    "AssemblPost": {
        "@extends": "Post"
    },
    "PostWithMetadata": {
        "@extends": "AssemblPost",
        "metadata_json": true
    },
    "IdeaProposalPost": {
        "@extends": "PostWithMetadata",
        "proposes_idea": true
    },
    "SynthesisPost": {
        "@extends": "AssemblPost",
        "publishes_synthesis": true
    },
    "ImportedPost": {
        "@extends": "Post",
        "source": true
    },
    "Webpage": {
        "@extends": "Content",
        "last_modified_date": true,
        "url": true
    },
    "Email": {
        "@extends": "ImportedPost",
        "recipients": false,
        "sender": true,
        "full_message": false,
        "in_reply_to": false
    },
    "Discussion": {
        "creation_date": true,
        "slug": true,
        "topic": true
    },
    "Idea": {
        "longTitle": "long_title",
        "shortTitle": "short_title",
        "definition": true,
        "is_tombstone": true,
        "creationDate": "creation_date",
        "order": "&get_order_from_first_parent",
        "discussion": false,
        "hidden": true,
        "parents": true,
        "parentId": "&get_first_parent_uri",
        "inNextSynthesis": "is_in_next_synthesis",
        "numChildIdea": "&get_num_children"
    },
    "Criterion": {
        "@extends": "Idea"
    },
    "Issue": {
        "@extends": "Idea"
    },
    "Argument": {
        "@extends": "Idea"
    },
    "Proposal": {
        "@extends": "Idea"
    },
    "RootIdea": {
        "@extends": "Idea",
        "root": "'true"
    },
    "IdeaLink": {
        "source": true,
        "target": true,
        "is_tombstone": true,
        "order": true
    },
    "IdeaGraphView": {
        "creation_date": true,
        "discussion": false
    },
    "ExplicitSubGraphView": {
        "@extends": "IdeaGraphView"
    },
    "TableOfContents": {
        "@extends": "IdeaGraphView"
    },
    "Synthesis": {
        "@extends": "ExplicitSubGraphView",
        "subject": true,
        "introduction": true,
        "conclusion": true
    },
    "IdeaContentLink": {
        "idea": true,
        "content": true,
        "creation_date": true,
        "creator": true
    },
    "IdeaContentPositiveLink": {
        "@extends": "IdeaContentLink"
    },
    "IdeaRelatedPostLink": {
        "@extends": "IdeaContentPositiveLink"
    },
    "IdeaContentNegativeLink": {
        "@extends": "IdeaContentLink"
    },
    "IdeaThreadContextBreakLink": {
        "@extends": "IdeaContentNegativeLink"
    },
    "Extract": {
        "idIdea": "idea",
        "idPost": "content",
        "created": "creation_date",
        "idCreator": "creator",
        "important": true,
        "owner": true,
        "quote": "body",
        "order": true,
        "discussion": false,
        "ranges": ["text_fragment_identifiers:"],
        "annotation_text": false
    },
    "TextFragmentIdentifier": {
        "end": "xpath_end",
        "endOffset": "offset_end",
        "extract_id": false,
        "start": "xpath_start",
        "startOffset": "offset_start"
    },
    "DiscussionPermission": {
        "discussion": true,
        "permission": "&permission_name",
        "role": "&role_name"
    },
    "ViewPost": {
        "actor": true,
        "post": true
    },
    "Widget": {
        "discussion": true,
        "settings": "settings_json",
        "state": "state_json"
    },
    "BaseIdeaWidget": {
        "@extends": "Widget"
    },
    "IdeaCreatingWidget": {
        "@extends": "BaseIdeaWidget"
    },
    "CreativitySessionWidget": {
        "@extends": "IdeaCreatingWidget"
    },
    "InspirationWidget": {
        "@extends": "IdeaCreatingWidget"
    },
    "MultiCriterionVotingWidget": {
        "@extends": "Widget"
    },
    "AbstractIdeaVote": {
        "idea": true,
        "criterion": true,
        "widget": true,
        "voter": true
    },
    "BinaryIdeaVote": {
        "@extends": "AbstractIdeaVote",
        "vote_value": true
    },
    "LickertIdeaVote": {
        "@extends": "AbstractIdeaVote",
        "vote_value": true
    },
    "PartnerOrganization": {
        "name": true,
        "description": false,
        "homepage": true,
        "logo": true,
        "is_initiator": true
    },
    "TimelineEvent": {
        "title": true,
        "description": true,
        "previous_event": true
    },
    "DiscussionPhase": {
        "@extends": "TimelineEvent",
        "start": true,
        "end": true
    },
    "DiscussionSession": {
        "@extends": "TimelineEvent",
        "start": true,
        "end": true
    },
    "DiscussionMilestone": {
        "@extends": "TimelineEvent",
        "start": true
    }
}
