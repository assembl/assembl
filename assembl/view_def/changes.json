{
    "_default": {
        "id": false,
        "type": false,
        "@id": "self",
        "@type": "&external_typename",
        "_default": false
    },
    "Permission": {
        "name": true
    },
    "AgentProfile": {
        "description": true,
        "preferred_email": false
    },
    "User": {
        "@extends": "AgentProfile",
        "creation_date": true,
        "last_login": true,
        "login_failures": false,
        "password": false,
        "verified": true
    },
    "UserTemplate": {
        "@extends": "User",
        "discussion": true,
        "for_role": true
    },
    "EmailAccount": false,
    "WebLinkAccount": false,
    "LoomioAccount": false,
    "IdentityProviderAccount": false,
    "IdentityProvider": {
        "name": true,
        "provider_type": true,
        "trust_emails": true
    },
    "ContentSource": {
        "discussion_id": "discussion",
        "name": true,
        "creation_date": true
    },
    "PostSource": {
        "@extends": "ContentSource",
        "last_import": true
    },
    "AbstractMailbox": {
        "@extends": "PostSource"
    },
    "AnnotatorSource": {
        "@extends": "ContentSource"
    },
    "Content": {
        "date": "creation_date",
        "hidden": true,
        "discussion": true,
        "import_date": false,
        "subject": true,
        "body": true
    },
    "Post": {
        "@extends": "Content",
        "message_id": false,
        "ancestry": false,
        "parentId": "parent",
        "idCreator": "creator"
    },
    "AssemblPost": {
        "@extends": "Post"
    },
    "PostWithMetadata": {
        "@extends": "AssemblPost",
        "metadata_json": false
    },
    "IdeaProposalPost": {
        "@extends": "PostWithMetadata",
        "proposes_idea": true
    },
    "SynthesisPost": {
        "@extends": "AssemblPost",
        "publishes_synthesis": true
    },
    "ImportedPost": {
        "@extends": "Post",
        "source": true
    },
    "FeedPost": {
        "@extends": "ImportedPost"
    },
    "LoomioFeedPost": {
        "@extends": "FeedPost"
    },
    "Email": {
        "@extends": "ImportedPost",
        "recipients": true,
        "sender": true,
        "full_message": false,
        "in_reply_to": false
    },
    "Webpage": {
        "@extends": "Content",
        "last_modified_date": true,
        "url": true
    },
    "IMAPMailbox": {
        "@extends": "AbstractMailbox",
        "folder": true,
        "host": true,
        "port": true,
        "username": false,
        "use_ssl": true,
        "password": false,
        "last_imported_email_uid": false,
        "subject_mangling_regex": false,
        "subject_mangling_replacement": false
    },
    "MailingList": {
        "@extends": "IMAPMailbox",
        "post_email_address": true
    },
    "Discussion": {
        "creation_date": true,
        "slug": true,
        "topic": true
    },
    "Idea": {
        "original_uri": true,
        "longTitle": "long_title",
        "shortTitle": "short_title",
        "definition": true,
        "creationDate": "creation_date",
        "order": "&get_order_from_first_parent",
        "discussion": false,
        "parents": true,
        "parentId": "&get_first_parent_uri",
        "inNextSynthesis": "is_in_next_synthesis",
        "numChildIdea": "&get_num_children",
        "num_posts": true,
        "num_read_posts": true
    },
    "RootIdea": {
        "@extends": "Idea",
        "root": "'true",
        "parentId": false,
        "num_orphan_posts": true,
        "num_synthesis_posts": true
    },
    "Criterion": {
        "@extends": "Idea"
    },
    "Issue": {
        "@extends": "Idea"
    },
    "Argument": {
        "@extends": "Idea"
    },
    "Proposal": {
        "@extends": "Idea"
    },
    "IdeaLink": {
        "original_uri": true,
        "target": true,
        "source": true,
        "order": true
    },
    "IdeaGraphView": {
        "creation_date": true,
        "discussion": false
    },
    "ExplicitSubGraphView": {
        "@extends": "IdeaGraphView"
    },
    "TableOfContents": {
        "@extends": "IdeaGraphView"
    },
    "Synthesis": {
        "@extends": "ExplicitSubGraphView",
        "subject": true,
        "introduction": true,
        "conclusion": true,
        "is_next_synthesis": true,
        "ideas": [":id_only"],
        "idea_links": [":partial"]
    },
    "IdeaContentLink": {
        "idea": true,
        "content": true,
        "creation_date": true,
        "creator": true
    },
    "IdeaContentPositiveLink": {
        "@extends": "IdeaContentLink"
    },
    "IdeaRelatedPostLink": {
        "@extends": "IdeaContentPositiveLink"
    },
    "IdeaContentNegativeLink": {
        "@extends": "IdeaContentLink"
    },
    "IdeaThreadContextBreakLink": {
        "@extends": "IdeaContentNegativeLink"
    },
    "Extract": {
        "idIdea": "idea",
        "idPost": "content",
        "target": true,
        "created": "creation_date",
        "idCreator": "creator",
        "important": true,
        "owner": true,
        "quote": "body",
        "order": true,
        "discussion": false,
        "ranges": ["text_fragment_identifiers:partial"],
        "text": "annotation_text"
    },
    "TextFragmentIdentifier": {
        "end": "xpath_end",
        "endOffset": "offset_end",
        "extract_id": false,
        "start": "xpath_start",
        "startOffset": "offset_start"
    }, 
    "DiscussionPermission": {
        "discussion": true,
        "permission": "&permission_name",
        "role": "&role_name"
    },
    "ViewPost": {
        "actor": true,
        "post": true
    },
    "PartnerOrganization": {
            "name": true,
            "description": true,
            "homepage": true,
            "logo": true,
            "is_initiator": true
    }
}
